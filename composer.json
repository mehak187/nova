{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "calliopee/booking-calendar": "*",
        "coderello/laravel-nova-lang": "^1.7",
        "eminiarts/nova-tabs": "^2.2",
        "firstpoint-ch/calliopee-styles": "*",
        "firstpoint-ch/camera-upload": "*",
        "firstpoint-ch/open-invoices": "*",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^9.0",
        "laravel/nova": "^4.32",
        "laravel/tinker": "^2.5",
        "silviolleite/laravelpwa": "^2.0",
        "simplesoftwareio/simple-qrcode": "~4",
        "spatie/laravel-ignition": "^1.4",
        "spatie/test-time": "^1.2",
        "titasgailius/search-relations": "^1.0",
        "whitecube/nova-flexible-content": "^1.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "path",
            "url": "./nova-components/BookingCalendar"
        },
        {
            "type": "path",
            "url": "./nova-components/OpenInvoices"
        },
        {
            "type": "path",
            "url": "./nova-components/CameraUpload"
        },
        {
            "type": "path",
            "url": "./nova-components/CalliopeeStyles"
        }
    ]
}
